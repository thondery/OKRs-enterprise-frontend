{
  "name": "flame-okrs-enterprise-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt-ts",
    "build": "run-s build:clean build:nuxt",
    "build:nuxt": "cross-env NODE_ENV=production nuxt-ts build",
    "build:clean": "rimraf .nuxt dist",
    "ci": "run-s test build",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "generate": "cross-env NODE_ENV=production nuxt-ts generate",
    "analyze": "nuxt build --analyze",
    "export": "nuxt-ts export",
    "serve": "nuxt-ts serve",
    "lint": "run-s lint:*",
    "lint:files": "ls-lint",
    "lint:eslint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --write --loglevel warn .",
    "test": "run-s test:unit lint",
    "test:unit": "cross-env NODE_ENV=test jest --passWithNoTests",
    "test:unit:debug": "cross-env NODE_ENV=test node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --passWithNoTests",
    "test:e2e": " cross-env NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env PRE_COMMIT=true ls-lint && lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/types": "^2.13.3",
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "element-ui": "^2.13.2",
    "js-cookie": "^2.2.1",
    "node-sass": "^4.14.1",
    "nuxt": "2.13.3",
    "nuxt-clipboard2": "^0.2.1",
    "sass-loader": "^9.0.2",
    "vue-class-component": "^7.2.5",
    "vue-fragment": "^1.5.1",
    "vue-property-decorator": "^9.0.0",
    "vuex-module-decorators": "^0.17.0"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ls-lint/ls-lint": "^1.9.0",
    "@nuxt/components": "^1.0.7",
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "@types/jest": "^26.0.4",
    "@types/js-cookie": "^2.2.6",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-component": "^1.1.1",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "imagemin-lint-staged": "^0.4.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "nuxt-purgecss": "^1.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^8.0.1",
    "typescript": "3.9.6",
    "vue-jest": "^3.0.4",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
    "yarn": "^1.22.4"
  }
}
